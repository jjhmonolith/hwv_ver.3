{
  "name": "hw-validator-backend",
  "version": "1.0.0",
  "description": "HW Validator ver.3 Backend API Server",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && cp src/db/schema.sql dist/db/",
    "start": "node dist/index.js",
    "migrate": "tsx src/db/migrate.ts",
    "migrate:prod": "node dist/db/migrate.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.33.0",
    "@supabase/supabase-js": "^2.93.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^6.16.0",
    "pdf-parse": "^1.1.4",
    "pg": "^8.11.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "pdfkit": "^0.17.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
